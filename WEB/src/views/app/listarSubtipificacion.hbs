<div id="main">
  <div class="row">
    <div class="col s12">
      <div class="container">
        <div class="section">
          <div class="row">
            <div class="col s12">
              <div class="card">
                <div class="card-content">
                  <h4 class="card-title" data-i18n="Lista de subtipificaciones">Lista de subtipificaciones</h4>
                  <div class="row">
                    <div class="col s12">
                      <table id="tuTablaSubtipificaciones" class="display">
                        <thead style="color: #fff">
                          <tr class="custom-gradient_black">
                            <th data-i18n="Categoria">Categoria</th>
                            <th data-i18n="Contenido">Contenido</th>
                            <th data-i18n="Accion">Acci√≥n</th>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each listaSubtipificacion}}
                          <tr>
                            <td>{{this.TIP_CNOMBRE_TIPIFICACION}}</td>
                            <td>{{this.OP_OPCION}}</td>
                            <td>
                              <a class="btn waves-effect waves-light color-oscuro" id="editar"
                                href="/subtipificaciones/actualizarSubtipificacion/{{this.EnCrypt}}"><i
                                  class='bx bx-edit-alt'></i></a>
                              <a class="btn waves-effect waves-light red accent-2" id="borrar"
                                eliminarSubtipificacion="{{this.EnCrypt}}"><i class='bx bx-trash-alt'></i></a>
                            </td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/js/listarSubtipificaciones.js"></script>